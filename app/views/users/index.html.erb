<% @users.each do |user| %>
	<% unless user == current_user %>
		<section class="window">
		  <h1><%= user.name %></h1>
		  <h3>Biography</h3>
		  <p><%= user.biography %></p>
		  <h3>Experience</h3>
		  <p><%= user.experience %></p>
		  <br>

		  <div id="follow_user">
		    <%= render :partial => "follows/follow_user", :locals => {:user => user} %>
		  </div>
  	</section>
	 <% end %>

<% end %>

	<% @follow.each do |follow| %>

		<% if current_user = follow.follower %>
			<%= follow.followable.name %>
		<% end %>
	<% end %>


