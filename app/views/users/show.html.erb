<h3><%= @user.first_name %></h3>
<h3><%= @user.last_name %></h3>


    
<h3><strong>Location: </strong><%= @user.location %></h3>

<h3><strong>Biography: </strong><%= @user.biography %></h3>
<h3><strong>Experience: </strong><%= @user.experience %></h3>


	<%= form_for @user do |f| %>
		  <div class="field">
		    <%= f.label :location %>
		    <%= f.text_area :location %>
		  </div>
		  <div class="field">
		    <%= f.label :biography %>
		    <%= f.text_area :biography %>
		  </div>
		   <div class="field">
		    <%= f.label :experience %>
		    <%= f.text_area :experience %>
		  </div>
		  <div class="action">
		    <%= f.submit %>
		  </div>
    <% end %>

