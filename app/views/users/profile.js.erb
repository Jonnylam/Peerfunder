$( "#editbutton" ).click(function() {
  $('#editform').html('<%= j render partial: 'users/addtoprofileinformation', locals: {user: @user} %>')
});

$( "#showeditbuttons" ).click(function() {
  $('.editbuttons').show();
  $('#canceleditbutton').show();
  $('#showeditbuttons').hide();
});

$('#canceleditbutton').click(function(){
  $('.editbuttons').hide();
  $('#showeditbuttons').show();
  $('#canceleditbutton').hide();
});



$("#editlocationbutton").click(function() {
  $('#edit_location_form').html('<%= j render partial: 'users/locationprofileinformation', locals: {user: @user} %>')
});

$("#editbiographybutton").click(function() {
  $('#edit_biography_form').html('<%= j render partial: 'users/biographyprofileinformation', locals: {user: @user} %>')
});

$("#editexperiencebutton").click(function() {
