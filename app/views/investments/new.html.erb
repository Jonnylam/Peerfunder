<div class="window">
  <h1>Make A New Investment</h1>

  <%= form_for ([@company, @round, @investment]) do |f| %>
  <h1>Create A New Round</h1>

  <div class="field">
    <%= f.label :amount%><br>
    <%= f.text_field :amount %><br>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
</div>

