<%= link_to 'Back to companies', companies_path %>

<%= form_for @company do |f| %>
  <h1>Add a New Company </h1>
  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %><br>
  </div>
    <br>
  <div class="field">
    <%= f.label :company_logo %><br>
    <%= f.text_field :company_logo %><br>
  </div>
    <br>
  <div class="field">
    <%= f.label :elevator_pitch %><br>
    <%= f.text_area :elevator_pitch %><br>
    <br>
  </div>
  <div class="field">
    <%= f.label :location %><br>
    <%= f.text_field :location %><br>
    <br>
  </div>
  <div class="field">
    <%= f.label :industry_type %><br>
    <%= f.text_field :industry_type %><br>
    <br>
  </div>
 	<div class="field">
    <%= f.label :website_url %><br>
    <%= f.text_field :website_url %><br>
    <br>
  </div>
<%= f.submit %>
<% end %>


